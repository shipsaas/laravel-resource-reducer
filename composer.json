{
    "name": "shipsaas/laravel-resource-reducer",
    "type": "library",
    "version": "1.0.0",
    "description": "Reduce fields & Fasten up your response size using Laravel Resource Reducer",
    "keywords": [
        "laravel library",
        "laravel",
        "laravel resource reducer",
        "laravel optimization"
    ],
    "authors": [
        {
            "name": "Phat Tran (Seth Phat)",
            "email": "me@sethphat.com",
            "homepage": "https://github.com/sethsandaru",
            "role": "Sr.SWE"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10|dev-master"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.20.0",
        "mockery/mockery": "^1.5.1",
        "phpunit/phpunit": "^10",
        "orchestra/testbench": "^8",
        "phpunit/php-code-coverage": "^10",
        "friendsofphp/php-cs-fixer": "^3.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShipSaasReducer\\ReducerServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ShipSaasReducer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShipSaasReducer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test-coverage": [
            "@php vendor/bin/phpunit --coverage-clover coverage.xml"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
